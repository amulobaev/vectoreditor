<UserControl x:Class="VectorEditor.Control.VectorEditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VectorEditor.Control"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <local:CustomCanvas x:Name="Canvas" x:FieldModifier="public" HorizontalAlignment="Left" VerticalAlignment="Top" Width="2720" Height="2720">
            
        </local:CustomCanvas>
    </ScrollViewer>
</UserControl>
